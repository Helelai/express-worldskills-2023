<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.session2.SignUpActivity">

    <ScrollView
        android:layout_width="391dp"
        android:layout_height="849dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/haveAccTWCrAcc2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_weight="1"
            android:fontFamily="@font/roboto"
            android:text="or sign in using"
            android:textColor="@color/grayHint"
            android:textSize="14dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/CreateAccountTW"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="33dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Create an account"
            android:textAlignment="center"
            android:textColor="#3A3A3A"
            android:textSize="24dp"
            app:lineHeight="30dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/CompleteTW"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Complete the sign up process to get started"
            android:textAlignment="center"
            android:textColor="@color/grayHint"
            android:textSize="14dp"
            app:lineHeight="16dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/FullNameTW"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="33dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Full name"
            android:textColor="@color/grayHint"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/FullNameET"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/round_bord_pltxt"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="Ivanov Ivan"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:paddingTop="14px"
            android:paddingRight="253px"
            android:singleLine="true"
            android:textColor="@color/blackDefault"
            android:textColorHint="@color/lightGrayHint"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/PhoneNumberTW"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Phone number"
            android:textColor="@color/grayHint"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Email Address"
            android:textColor="@color/grayHint"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Password"
            android:textColor="@color/grayHint"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/PhoneNumberET"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/round_bord_pltxt"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="+7(999)999-99-99"
            android:inputType="phone"
            android:paddingLeft="10dp"
            android:paddingTop="14px"
            android:paddingEnd="253px"
            android:paddingBottom="14px"
            android:textColor="@color/blackDefault"
            android:textColorHint="@color/lightGrayHint"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/EmailET"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/round_bord_pltxt"
            android:ems="10"
            android:fontFamily="@font/roboto_medium"
            android:hint="***********@mail.com"
            android:inputType="textEmailAddress"
            android:paddingLeft="10dp"
            android:paddingTop="14px"
            android:paddingRight="253px"
            android:paddingBottom="14px"
            android:textColor="@color/blackDefault"
            android:textColorHint="@color/lightGrayHint"
            tools:ignore="MissingConstraints" />

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/round_bord_pltxt"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/PasswordET"
                android:layout_width="310dp"
                android:layout_height="44dp"
                android:background="@null"
                android:ems="10"
                android:hint="**********"
                android:inputType="textPassword"
                android:paddingLeft="10dp"
                android:paddingTop="14px"
                android:paddingRight="10px"
                android:paddingBottom="14px"
                android:textColorHint="@color/lightGrayHint" />

            <ImageButton
                android:id="@+id/HideShowPassB"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@null"
                android:onClick="HideShowPassBClick"
                app:srcCompat="@drawable/ic_eye_off" />

        </LinearLayout>

        <TextView
            android:id="@+id/ConfirmPasswordTW"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/roboto_medium"
            android:text="Confirm Password"
            android:textColor="@color/grayHint"
            tools:ignore="MissingConstraints" />

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/round_bord_pltxt"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <EditText
                android:id="@+id/ConfirmPasswordET"
                android:layout_width="310dp"
                android:layout_height="44dp"
                android:background="@null"
                android:ems="10"
                android:hint="**********"
                android:inputType="textPassword"
                android:paddingLeft="10dp"
                android:paddingTop="14px"
                android:paddingRight="10px"
                android:paddingBottom="14px"
                android:textColorHint="@color/lightGrayHint" />

            <ImageButton
                android:id="@+id/HideShowConfirmPassB"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@null"
                android:onClick="HideShowConfirmPassBClick"
                app:srcCompat="@drawable/ic_eye_off" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="69dp"
            android:orientation="horizontal"
            android:textAlignment="center"
            tools:ignore="MissingConstraints">

            <CheckBox
                android:id="@+id/TermsCHBX"
                android:layout_width="14dp"
                android:layout_height="14dp"
                android:height="14dp"
                android:background="@drawable/image_checkbox"

                android:button="@android:color/transparent"
                android:checked="true"
                android:fontFamily="@font/roboto"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/terms_and_condition"
                android:textAlignment="center"
                android:textSize="12dp"
                app:lineHeight="16dp" />
        </LinearLayout>

        <Button
            android:id="@+id/SignUpBCrAcc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:width="342dp"
            android:height="46dp"
            android:backgroundTint="@color/titleBlue"
            android:fontFamily="@font/roboto_bold"
            android:paddingLeft="146px"
            android:paddingTop="15px"
            android:paddingRight="146px"
            android:paddingBottom="15px"
            android:text="Sign Up"
            app:cornerRadius="4dp"
            tools:ignore="MissingConstraints" />

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="18dp"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/HaveAccTWCrAcc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/roboto"
                android:text="Already have an account?"
                android:textColor="@color/grayHint"
                android:textSize="14dp" />

            <TextView
                android:id="@+id/SignInBCrAcc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/roboto_medium"
                android:text="Sign in"
                android:textColor="@color/titleBlue"
                android:textSize="14dp" />

        </LinearLayout>

        <ImageButton
            android:id="@+id/GoogleSignInB"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="28dp"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/ic_google"
            tools:ignore="MissingConstraints" />
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>